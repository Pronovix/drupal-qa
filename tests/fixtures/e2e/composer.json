{
    "name": "pronovix/drupal-qa-e2e",
    "description": "Drupal QA end-to-end test suite",
    "license": "GPL-2.0-or-later",
    "type": "library",
    "require": {
        "mglaman/drupal-check": "^1.4.0",
        "pronovix/drupal-qa": "*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "..",
            "options": {
                "symlink": false
            }
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "pronovix/drupal-qa": true
        },
        "platform": {
            "php": "8.3.0"
        }
    },
    "extra": {
        "composer-normalize": {
            "indent-size": 4,
            "indent-style": "space"
        },
        "monorepo-helper": {
            "enabled": false
        }
    }
}
