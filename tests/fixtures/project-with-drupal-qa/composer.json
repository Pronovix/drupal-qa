{
    "name": "pronovix/drupal-qa-acme",
    "type": "project",
    "require": {
        "pronovix/drupal-qa": "^99.99999"
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/fake_drupal_qa"
        }
    ],
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    },
    "extra": {
        "composer-normalize": {
            "indent-size": 4,
            "indent-style": "space"
        },
        "monorepo-helper": {
            "enabled": false
        }
    }
}
