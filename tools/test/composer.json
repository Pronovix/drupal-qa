{
    "name": "pronovix/drupal-qa-testing-tools",
    "description": "Drupal QA testing tools",
    "license": "GPL-2.0-or-later",
    "type": "library",
    "require": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "platform": {
            "php": "8.1.6"
        }
    },
    "extra": {
        "composer-normalize": {
            "indent-size": 4,
            "indent-style": "space"
        }
    }
}
